#include <stdio.h>
#include <string.h>
#include <time.h>

#define MAX_COMMANDS 10
#define COMMAND_LEN 15

int readline(char str[], int n);

char commands[MAX_COMMANDS][COMMAND_LEN + 1]


int main(void) {
  
  return 0;
}


int readline(char str[], int n) {
  int ch, i = 0;

  while ((ch = getchar()) != '\n')
    if (i < n)
      str[i++] = ch;
  str[i] = '\0';
  return i;
}
